<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="vgr-blandbarhet-theme" name="Blandbarhet Theme">
		<template-extension>ftl</template-extension>
		<settings>
			<setting key="portlet-setup-show-borders-default" value="true" configurable="true" type="checkbox" />
			<setting key="additional-logo-class" value="rp" configurable="true" type="select" options="rp,vgr-portal,blandbarhet,wwwprv" />
			<setting key="global-info-header" value="" configurable="true" type="text" />
		</settings>

	</theme>
</look-and-feel>
